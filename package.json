    "@types/react-redux": "^7.1.0",
    "@types/react-router": "^5.0.2",
    "@types/react-router-dom": "^4.3.4",
    "@types/react-table": "^6.8.4",
    "@types/redux-saga": "^0.10.5",
    "axios": "^0.19.0",
    "classnames": "^2.2.6",
    "connected-react-router": "^6.4.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "enzyme-to-json": "^3.3.5",
    "highcharts": "^7.2.2",
    "highcharts-react-official": "^2.2.2",
    "history": "^4.9.0",
    "i18next": "^17.0.3",
    "i18next-xhr-backend": "^3.0.0",
    "lodash": "^4.17.14",
    "node-sass": "^4.12.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-i18next": "^10.11.1",
    "react-redux": "^7.1.0",
    "react-router": "^5.0.1",
    "react-router-dom": "^5.0.1",
    "react-scripts": "3.0.1",
    "react-table": "^6.10.0",
    "redux-saga": "^1.0.3",
    "reselect": "^4.0.0",
    "typesafe-actions": "^4.4.1",
    "typescript": "3.4.5"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --transformIgnorePatterns \"node_modules/(?!(react-i18next/hooks))/\"",
    "test:coverage": "yarn test --coverage",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "coverageReporters": [
      "text",
      "lcov"
    ],
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!node_modules/",
      "!src/typings/**/*.*",
      "!src/jest/**/*.*",
      "!src/index.tsx",
      "!src/**/index.ts",
      "!src/common/api/entities/*.*",
      "!src/common/enums/*.*",
      "src/store/store.ts",
      "!src/services/translation-service.ts",
      "!src/services/elf-service.ts",
      "!src/services/api-service.ts",
      "!src/styles/**/*.*"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  }
}
